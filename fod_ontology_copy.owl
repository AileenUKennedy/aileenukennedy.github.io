###  http://www.w3.org/ns/sosa/resultTime
<http://www.w3.org/ns/sosa/resultTime> rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://www.marine.ie/ontologies/fod#Area
:Area rdf:type owl:Class ;
      rdfs:comment "The area that this LandingEvent was caught in."@en ;
      rdfs:label "Area"@en .


###  http://www.marine.ie/ontologies/fod#ArrivalLocation
:ArrivalLocation rdf:type owl:Class ;
                 rdfs:comment "LOCODE of arrival location"@en ;
                 rdfs:label "Arrival Location"@en .


###  http://www.marine.ie/ontologies/fod#BioVar
:BioVar rdf:type owl:Class ;
        rdfs:subClassOf <http://www.w3.org/ns/sosa/Observation> ;
        rdfs:comment "Biological variable level ID"@en ;
        rdfs:label "Biological variable" .


###  http://www.marine.ie/ontologies/fod#BioVarFish
:BioVarFish rdf:type owl:Class ;
            rdfs:comment "National ID of the Individual fish / specimen. Unique across all species, years etc. This could be use as the link to other system e.g. SmartDots." ;
            rdfs:label "National unique fish ID" .


###  http://www.marine.ie/ontologies/fod#CatchCategory
:CatchCategory rdf:type owl:Class ;
               rdfs:comment """'Lan': Landings categorised as above minimum reference size
'BMS': landings categorised as below minimum reference size
'Catch': catch which is not determined if it is landing or discard so it include potentially both
'Dis': discard released to the sea"""@en ;
               rdfs:label "Catch category"@en .


###  http://www.marine.ie/ontologies/fod#CatchFraction
:CatchFraction rdf:type owl:Class ;
