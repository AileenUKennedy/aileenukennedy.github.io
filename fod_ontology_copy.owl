<http://www.marine.ie/ontologies/fod> rdf:type owl:Ontology ;
                                       terms:created "2022-05"@en ;
                                       terms:creator "Aileen Kennedy" ;
                                       terms:description "This ontology is based on fisheries observational data, in particular hierarchical sampling such as that used by ICES."@en ;
                                       terms:license "https://creativecommons.org/licenses/by-nc-sa/2.0/" ;
                                       terms:title "Fisheries Observational Data (FOD) Ontology"@en ;
                                       vann:preferredNamespacePrefix "fod" .

