                                                                  rdfs:comment "Relation linking Design  and Sampling scheme type"@en ;
                                                                  rdfs:label "has sampling scheme type"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.marine.ie/ontologies/fod#hasArrivalDate
:hasArrivalDate rdf:type owl:DatatypeProperty ;
                rdfs:comment "has arrival date"@en ;
                rdfs:label "has arrival date"@en .


###  http://www.marine.ie/ontologies/fod#hasClustering
:hasClustering rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:boolean ;
               rdfs:comment "Indication of presence or absence of clustering"@en ;
